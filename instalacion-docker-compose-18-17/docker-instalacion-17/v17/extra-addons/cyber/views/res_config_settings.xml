<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_general_config_cyber_categoria" model="ir.ui.view">
        <field name="name">Configuration: add Cyber saymon options</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base_setup.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <div id="contacts_settings" position="before">
                <h2>Cyber Categoria</h2>
                <div class="row mt16 o_settings_container">
                    <!-- Release Dates option -->
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                            <field name="group_cyber_description" class="oe_inline"/>
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="group_cyber_description"/>
                            <div class="text-muted">
                                Enabledescricpion en categorias para cyber
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </field>
    </record>
</odoo>