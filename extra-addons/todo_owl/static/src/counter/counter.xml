<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">

    <t t-name="todo_owl.counter">
         <p>Sonnyra Counter: <t t-esc="state.value"/></p>
         <p>Sonnyra a: <t t-esc="props.a"/></p>
         <p>Sonnyra b: <t t-esc="props.b"/></p>
          <button class="btn btn-primary" t-on-click="increment">Increment saymons</button>
           <span type="button"
          class="p-2"
          t-on-click="() => this.props.callback(state.value)">
          Enviar al papa
         </span>

    </t>

</templates>
