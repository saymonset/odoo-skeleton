<data>
    <template id="report_picking" inherit_id="stock.report_picking">
        <xpath
            expr="//span[@t-if='ml.move_id.product_packaging_id']/span[2]/span[@t-field='ml.product_packaging_qty']"
            position="attributes"
        >
            <attribute name="t-field">ml.product_packaging_quantity</attribute>
        </xpath>
    </template>

    <template
        id="stock_report_delivery_has_serial_move_line"
        inherit_id="stock.stock_report_delivery_has_serial_move_line"
    >
        <xpath
            expr="//span[@t-field='move_line.product_packaging_qty']"
            position="attributes"
        >
            <attribute name="t-field">move_line.product_packaging_quantity</attribute>
        </xpath>
    </template>
</data>
