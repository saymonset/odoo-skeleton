<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.vertical.lift</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="70" />
        <field name="inherit_id" ref="base.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <block name="warehouse_setting_container" position="after">
                <block name="vertical_lift_setting_container" title="Vertical Lift">
                    <setting
                        id="vertical_lift_secret"
                        title="Secret for Vertical Lift API"
                    >
                        <field name="vertical_lift_secret" />
                    </setting>
                </block>
            </block>
        </field>
    </record>
</odoo>
